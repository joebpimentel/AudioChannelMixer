<Window x:Class="AudioChannelMixer.Shell"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:prism="http://www.codeplex.com/prism"
        xmlns:inf="clr-namespace:AudioChannelMixer.Infrastrucure;assembly=AudioChannelMixer.Infrastrucure"
        mc:Ignorable="d"
        Title="Audio Channel Mixer" 
        Height="450" Width="800">
    <DockPanel LastChildFill="True">
        <ContentControl prism:RegionManager.RegionName="{x:Static inf:RegionNames.ContentRegion}" />
    </DockPanel>
</Window>
